{
  "name": "DealScore",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "npm": ">=4",
    "node": ">=7"
  },
  "author": "Domenico Colandrea",
  "repository": {
    "type": "git",
    "url": "https"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "postinstall": "remotedev-debugger",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "clean": "rimraf $TMPDIR/react-* && watchman watch-del-all && npm cache clean",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rimraf $TMPDIR/react-* && watchman watch-del-all && rimraf ios/build && rimraf node_modules/ && npm cache clean"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "pre-commit": "lint:staged",
  "lint-staged": {
    "*.js": [
      "lint:eslint",
      "prettier --single-quote --trailing-comma all --write",
      "git add"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "5.0.0",
    "axios": "0.16.2",
    "color": "0.11.3",
    "expo": "17.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "native-base": "2.1.5",
    "react": "16.0.0-alpha.6",
    "react-native": "0.44.0",
    "react-native-camera-roll-picker": "^1.1.9",
    "react-native-gifted-chat": "^0.1.4",
    "react-native-modalbox": "^1.3.9",
    "react-native-multi-slider": "^0.3.5",
    "react-native-router-flux": "^3.39.2",
    "react-native-scrollable-tab-view": "^0.6.6",
    "react-native-swiper": "^1.5.4",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-persist": "^4.8.0",
    "redux-saga": "^0.15.3",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-react-native": "^1.9.2",
    "chai": "^4.0.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-prettier": "^2.1.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-redux-saga": "^0.3.0",
    "jest": "^20.0.4",
    "jest-expo": "~1.0.1",
    "jest-react-native": "^18.0.0",
    "lint-staged": "^3.6.0",
    "mocha": "^3.4.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.4.3",
    "react-native-scripts": "0.0.30",
    "react-test-renderer": "16.0.0-alpha.6",
    "remote-redux-devtools": "^0.5.11",
    "remote-redux-devtools-on-debugger": "^0.7.1",
    "rimraf": "^2.6.1"
  }
}
